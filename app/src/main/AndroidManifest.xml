    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".database.facturasAPP"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.JCCV_1"
        tools:targetApi="31">
        <activity
            android:name=".practica2.SwapperActivity"
            android:theme="@style/AppTheme"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.MainActivity"
            android:theme="@style/AppTheme"
            android:exported="false"/>
        <activity android:name=".activities.SecondaryActivity"
                  android:theme="@style/AppTheme"
                android:exported="false" />
        <activity android:name=".practica2.FirstFragment"
            android:theme="@style/AppTheme"
            android:exported="false" />

        <activity android:name=".practica2.Actividad2"
            android:theme="@style/AppTheme"
            android:exported="false" />





    </application>
</manifest>